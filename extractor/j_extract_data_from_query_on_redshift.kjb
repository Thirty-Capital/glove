      <description/>
      <type>SQL</type>
      <attributes/>
      <sql>BEGIN;
	CREATE TABLE #${SCHEMA_NAME}_${TABLE_NAME} AS ${INPUT_DATA_STATEMENT};
	UNLOAD ( 'SELECT * FROM #${SCHEMA_NAME}_${TABLE_NAME};' ) TO 's3://${GLOVE_STORARE_BUCKET_STAGING}/unload/${SCHEMA_NAME}/${TABLE_NAME}/'
		${REDSHIFT_UNLOAD_COPY_AUTHENTICATION}
		HEADER
		DELIMITER ';'
		NULL AS ''
		ESCAPE
		${QUOTES}
		GZIP
		ALLOWOVERWRITE
		PARALLEL ON;
END; </sql>
