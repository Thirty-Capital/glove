      <script>error_check(){
	if [ $? -gt 0 ]; then
       	 	echo "An error has occurred :_("

        	# Remove os arquivos temporários.
        	if [ ${DEBUG} = 0 ] ; then
            		rm -rf ${RAWFILE_QUEUE_PATH}*
       	 	fi

		exit 1
	fi
}

QUERY_ID=`aws athena start-query-execution --query-string "${INPUT_DATA_STATEMENT}" --output text --result-configuration OutputLocation=s3://${STORAGE_BUCKET}/staging`
error_check

echo "Running query $QUERY_ID!"

while true
do
    QUERY_STATUS=`aws athena get-query-execution --query-execution-id $QUERY_ID | jq '.QueryExecution.Status.State' | sed 's/\"//g'`
    error_check

    if [[ "$QUERY_STATUS" == "SUCCEEDED" ]]; then
       QUERY_RESULTSET=`aws athena get-query-execution --query-execution-id $QUERY_ID | jq '.QueryExecution.ResultConfiguration.OutputLocation' | sed 's/\"//g'`
	error_check
	break
    elif [[ $QUERY_STATUS == "FAILED" || $QUERY_STATUS == "CANCELLED" ]]; then
       echo "Query  $QUERY_ID failed!"
	echo `aws athena get-query-execution --query-execution-id $QUERY_ID | jq '.QueryExecution.Status.StateChangeReason'`
	break
    else
        sleep 10
    fi
done

if [ -z $QUERY_RESULTSET ]; then
	exit 1
else
	echo "Script sucessfully"
fi</script>
